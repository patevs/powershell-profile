{
  "name": "powershell-profile",
  "version": "0.16.2",
  "description": "PowerShell Script Template",
  "author": "PatEvs",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/patevs/powershell-profile.git"
  },
  "scripts": {
    "updates": "npx ncu",
    "release": "np"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.0.1",
    "commitlint-config-gitmoji": "^2.2.5",
    "husky": "^4.3.8"
  },
  "commitlint": {
    "extends": [
      "gitmoji"
    ],
    "rules": {
      "type-empty": [
        0,
        "never"
      ],
      "subject-empty": [
        0,
        "never"
      ]
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
